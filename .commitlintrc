{
  "extends": [
    "@commitlint/config-conventional"
  ],
  "rules": {
    "header-max-length": [
      0,
      "always",
      144
    ],
    "type-enum": [
      "build",
      "chore",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ]
  }
}
